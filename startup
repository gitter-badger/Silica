
os.loadAPI('src/debug')
_G.debug = debug

local h = fs.open('log', 'w')
h.write('')
h.close()

local _print = print
function _G.print(text)
	_print(text)
	log(tostring(text))
end

function _G.log( text )
	local h = fs.open('log', 'a')
	h.write(tostring(text) .. '\n')
	h.close()	
end

function _G.log2( text )
	local h = fs.open('log2', 'a')
	h.write(tostring(text) .. '\n')
	h.close()	
end

os.loadAPI('src/Silica')

TestApplication():run( ... )
